.docs-route {
  code {
    background #ffc8b4;
    border 1px solid darken(#ffc8b4, 10%);
    padding 1px 4px;
    border-radius 3px;
  }

  pre {
    background #dedede;
    border 1px solid darken(#dedede, 10%);
    border-radius 3px;
    overflow auto;
    padding 5px 7px;
    code {
      background none;
      border none;
      padding 0;
      border-radius 0;
    }
  }
}